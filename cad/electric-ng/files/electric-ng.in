#!/bin/csh
#
# $MBSDlabs$
# $FreeBSD$

set JAVA = "%%LOCALBASE%%/bin/java"
set LIBFILES = "%%LIBFILES%%"
set JARPATH = "%%JAVALIBDIR%%"

if ( "${LIBFILES}" != "" ) then
	set CLASSPATH = :`echo ${LIBFILES} | sed -E -e 's,[[:space:]]+,:,g'`
else
	set CLASSPATH = ""
endif

exec "${JAVA}" -cp %%JAVAJARDIR%%/%%JARFILE%%"${CLASSPATH}" com.sun.electric.Launcher
