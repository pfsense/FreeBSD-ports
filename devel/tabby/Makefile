	@${REINPLACE_CMD} \
		-e "s|https://github.com/swagger-api/swagger-ui/archive/refs/tags/v5.17.14.zip|file://${DISTDIR}/${DIST_SUBDIR}/v5.17.14.zip|" \
		${WRKSRC}/cargo-crates/utoipa-swagger-ui-9.0.1/build.rs

