		<maintainer>dv_serg@mail.ru</maintainer>
		<build_options>lightsquid_SET_FORCE=GD;libgd_UNSET_FORCE=FONTCONFIG XPM;perl_UNSET_FORCE=MULTIPLICITY</build_options>
		<status>RC</status>
