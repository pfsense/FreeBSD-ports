[
{ type: install
  message: <<EOM
 Please note that Thinkingrock will not work with diablo-jdk16
 or diablo-jre16. Set the JAVA_HOME variable to a different
 Java installation:

 e.g. tcsh:
 > setenv JAVA_HOME /usr/local/jdk1.6.0
 > thinkingrock

 e.g. sh:
 $ export JAVA_HOME=/usr/local/diablo-jre1.5.0
 $ thinkingrock
EOM
}
]
